<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">

    <!--代表外部存储区域的根目录下的文件 Environment.getExternalStorageDirectory()/DCIM/cameraDemo目录-->
    <external-path name="hm_DCIM" path="DCIM/cameraDemo" />
    <!--代表外部存储区域的根目录下的文件 Environment.getExternalStorageDirectory()/Pictures/cameraDemo目录-->
    <external-path name="hm_Pictures" path="Pictures/cameraDemo" />
    <!--代表app 私有的存储区域 Context.getFilesDir()目录下的images目录 /data/user/0/com.hm.camerademo/files/privateFiles-->
    <files-path name="hm_private_files" path="privateFiles" />
    <!--代表app 私有的存储区域 Context.getCacheDir()目录下的images目录 /data/user/0/com.hm.camerademo/cache/privateCaches-->
    <cache-path name="hm_private_cache" path="privateCaches" />
    <!--代表app 外部存储区域根目录下的文件 Context.getExternalFilesDir(Environment.DIRECTORY_PICTURES)目录下的Pictures目录-->
    <!--/storage/emulated/0/Android/data/com.hm.camerademo/files/externalFiles-->
    <external-files-path name="hm_external_files" path="externalFiles" />
    <!--代表app 外部存储区域根目录下的文件 Context.getExternalCacheDir目录下的images目录-->
    <!--/storage/emulated/0/Android/data/com.hm.camerademo/cache/externalCaches-->
    <external-cache-path name="hm_external_cache" path="externalCaches" />
</paths>